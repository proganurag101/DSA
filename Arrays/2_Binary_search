#include<iostream>
#include<math.h>
using namespace std;
int binary_search(int arr[],int n,long int key){
	int s = 0;
	int e = n-1;
	while(s<=e){
		int mid = (s+e)/2;
		if(arr[mid]==key){
			return mid;
		}else if(arr[mid]>key){
			e = mid-1;
		}else{
			s = mid+1;
		}
	}

    return 0;
}






int main(){
int arr[]{10,20,30,0,50,60,70,89};
int n = sizeof(arr)/sizeof(int);
int k{};
cin>>k;

int val = binary_search(arr,n,k);


if(val!=0){
    cout<<"index of key: "<<val<<endl;
}else{
    cout<<"key is not found!"<<endl;
}



    return 0;
}

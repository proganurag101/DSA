#include<iostream>
#include<math.h>
using namespace std;
int linear_search(int arr[],int n,int k){
    for(int i{};i<n;i++){
        if(arr[i]==k){
            return i;
        }
    }
    return 0;
}






int main(){
int arr[]{12,23,4232,233,22,0,34};
int n = sizeof(arr)/sizeof(int);
int k{};
cin>>k;

int val = linear_search(arr,n,k);
if(val!=0){
    cout<<"index of key: "<<val<<endl;
}else{
    cout<<"key is not found!"<<endl;
}



    return 0;
}
